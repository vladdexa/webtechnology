{
   "compilerOptions": {
     "lib": [
       "es5",
       "es6",
       "dom"
     ],
     "allowJs": true,
     "target": "es5",
     "module": "commonjs",
     "moduleResolution": "node",
     "outDir": "./build",
     "rootDir": "",
     "strict": true,
     "noImplicitAny": true,
     "esModuleInterop": true,
     "resolveJsonModule": true,
     "emitDecoratorMetadata": true,
     "experimentalDecorators": true,
     "sourceMap": true,
     "forceConsistentCasingInFileNames": true,
     "strictPropertyInitialization": false
   }
 }
